{
  "name": "Craigslist Helper",
  "version": "0.0.1",
  "description": "Utilities to make craigslist buying easier",
  "content_scripts": [
    {
      "matches": ["*://*.craigslist.org/*/*", "*://*.craigslist.org/search/*"],
      "js": ["cl_emailer.js"],
      "run_at": "document_end"
    }
  ],
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },
  "manifest_version": 2
}
